<Window x:Class="DogShowProgram.Windows.Authorization_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DogShowProgram.Windows"
        mc:Ignorable="d"
        Title="Authorization_Window" Height="585" Width="465" Loaded="Window_Loaded"
        WindowStartupLocation="CenterScreen" WindowStyle="None" ResizeMode="NoResize">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF242F9B" Offset="0"/>
            <GradientStop Color="#FFDBDFFD" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Frame x:Name="Frame_main"/>
    </Grid>
</Window>
